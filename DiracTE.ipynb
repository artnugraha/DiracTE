    "plt.text(-0.7,1.0, r'$r_\\kappa = 1$', color='r')\n",
    "plt.text(-0.7,0.2, r'$r_\\kappa = 2$', color='g')\n",
    "# in plt.xlabel or ylabel, the option r is not necessary\n",
